@{
}

@model IEnumerable<string>

<div class="mb-1">
    <a asp-controller="Home"
       asp-action="Index"
       asp-route-bookCategory=""
       asp-route-pageNum="1">Home</a>
</div>

@foreach (string category in Model)
{
    <div class="mb-1">
        <a asp-controller="Home"
           asp-action="Index"
           asp-route-bookCategory="@category"
           asp-route-pageNum="1">@category</a>
    </div>
}